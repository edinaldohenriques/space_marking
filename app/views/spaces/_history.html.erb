<%= form_with url: reservation_history_space_path(@space, format: :pdf), method: :get, html: { target: "_blank" }, remote: true do |f| %>
  <!-- Campo de data inicial -->
  <div>
    <%= f.label :start_date, "Data Inicial:", id: "label-start-date-pdf" %>
    <%= f.date_field :start_date, id: "start-date-pdf", placeholder: "dd/mm/aaaa", value: params[:start_date] %>
  </div>

  <!-- Campo de data final -->
  <div>
    <%= f.label :end_date, "Data Final:", id: "label-end-date-pdf" %>
    <%= f.date_field :end_date, id: "end-date-pdf", placeholder: "dd/mm/aaaa", value: params[:end_date] %>
  </div>

  <%= f.submit "Gerar PDF", class: "button-pdf" %>
<% end %>
